{
  "name": "my-portfolio-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:live": "env $(cat .env.local | xargs) next dev",
    "dev:dev": "env $(cat .env.dev | xargs) next dev",
    "prebuild": "node scripts/generate-external-manifest",
    "validate-manifest": "node scripts/validate-manifest.js",
    "build": "npm run validate-manifest && next build",
    "start": "next start",
    "lint": "eslint",
    "migrate-storage": "node scripts/migrate-storage-source.js",
    "migrate-storage:dry-run": "node scripts/migrate-storage-source.js --dry-run",
    "generate-external-manifest": "node scripts/generate-external-manifest.js",
    "generate-external-manifest:help": "node scripts/generate-external-manifest.js --help",
    "init-external-local": "node scripts/generate-external-manifest.js --init-local"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.90.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.3.1",
    "next": "16.1.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "rss-parser": "^3.13.0",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
